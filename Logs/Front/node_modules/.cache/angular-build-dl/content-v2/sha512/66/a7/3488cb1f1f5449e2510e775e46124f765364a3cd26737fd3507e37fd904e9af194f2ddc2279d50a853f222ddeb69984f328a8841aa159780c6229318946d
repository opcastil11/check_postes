onImgClick(value: string) {\n    this.currentLog = this.logs_to_show.filter((log) => log.id === value)[0];\n    this.displayModal = true;\n  }\n\n  onCloseClick() {\n    this.displayModal = false;\n    this.currentLog = undefined;\n  }\n\n  formatId(value: string){\n    return \"link\";\n  };\n\n  SetfilterLogsByCamera(value: any){\n    this.filterByName = value;\n    this.filterLogs();\n  };\n\n\n  filterLogs(){\n    if (this.filterByName === \"ALL\"){\n      var filtered =  this.logs;\n      this.button_camera = \"Camera\";\n    }else{\n      var filtered = this.logs_to_show.filter(log => log.camera == this.filterByName);\n      this.button_camera = this.filterByName;\n    }\n    this.logs_to_show = filtered;\n  };\n\n  formatLabel(value: string){\n//var class_tf = [\"person\", \"car\", \"bicycle\", \"motorbike\", \"bus\", \"truck\"]\n    if (value == 'car'){\n      value = 'Detección de Auto'\n    }else if (value == 'person'){\n      value = 'Detección de Persona'\n    }else if (value == 'bicycle'){\n      value = 'Detección de Bicicleta'\n    }else if (value == 'motorbike'){\n      value = 'Detección de Motocicleta'\n    }else if (value == 'bus'){\n      value = 'Detección de Bus'\n    }else if (value == 'truck'){\n      value = 'Detección de Camión'\n    }else{\n      value = 'Detección'\n    }\n    return value;\n  };\n\n  formatDate(value: string){\n    var d = new Date(value)\n    var seconds = d.getSeconds()\n    var minutes = d.getMinutes()\n    var hour = d.getHours()\n    var month = (d.getMonth() + 1)\n    var day = d.getDate()\n    var year = d.getFullYear()\n    var seconds_str = String(seconds);\n    var minutes_str = String(minutes);\n    var hour_str = String(hour);\n    var day_str = String(day);\n    var month_str = String(month);\n\n    if (seconds<10){\n        seconds_str=\"0\"+seconds;\n        }\n    if (minutes<10){\n        minutes_str=\"0\"+minutes;\n        }\n    if (hour<10){\n        hour_str=\"0\"+hour\n        }\n    if (month < 10) \n        month_str = '0'+month;\n    if (day < 10) \n        day_str = '0'+day;\n    //return value\n    return [year, month_str, day_str].join('-')+\" \"+hour_str+\":\"+minutes_str+\":\"+seconds_str;\n  };\n\n  linkVideo(value: string){\n    if (value == ''){\n      return ''\n    }else{\n      return \"link\";\n    }\n  };\n  pathVideo(value: string){\n    if (value == ''){\n      return \"#\"\n    }else{\n      return \"http://localhost:9091/videos/\"+value;\n    }\n  };\n  pathImage(value: string){\n    if (value == ''){\n      return \"http://localhost:9091/images/no_image.png\"\n    }else{\n      return \"http://localhost:9091/images/\"+value;\n    }\n  };\n  errorImage(image: any){\n    image.src = \"http://localhost:9091/images/no_image.png\"\n  };\n}\n","<div class=\"container\">\n  <nav class=\"navbar\">\n    <div class=\"navbar-item\">\n      <h1 class=\"title is-4\" style=\"margin-right:10px;\">Display Events Detected\n      </h1>\n      <div class=\"col\">\n          <div ngbDropdown class=\"d-inline-block\">\n            <button class=\"btn btn-outline-primary\" id=\"dropdownBasic1\" ngbDropdownToggle>{{button_camera}}</button>\n            <div ngbDropdownMenu aria-labelledby=\"dropdownBasic1\" style=\"overflow-y: scroll; height: 200px;\">\n              <tr *ngFor=\"let camera of cameras\">\n                <button ngbDropdownItem (click)=\"SetfilterLogsByCamera(camera)\">{{camera}}</button>\n            </tr>\n            </div>\n          </div>\n      </div>\n      <h4 style=\"margin-left:100px;\"> \n        <a href=\"http://186.103.144.163:9091/download\" download>\n          Download all logs\n        </a>\n      </h4>\n    </div>\n  </nav>\n  <table class=\"table is-fullwidth is-hoverable is-bordered\">\n    <thead>\n      <tr>\n        <th>ID Log</th>\n        <th>Date</th>\n        <th>Camera</th>\n        <th>Event</th>\n        <th>Image</th>\n        <th>Video</th>\n      </tr>\n    </thead>\n    <tbody>\n      <tr *ngFor=\"let log of lo